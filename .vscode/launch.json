{
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Deno",
            "program": "${workspaceFolder}/API/index.ts",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "deno",
            "outputCapture": "std",
            "envFile": "${workspaceFolder}/.env",
            "runtimeArgs": [
                "run",
                "--inspect-brk=127.0.0.1:9229",
                "--allow-all"
            ],
            "attachSimplePort": 9229
        }
        
    ]
}